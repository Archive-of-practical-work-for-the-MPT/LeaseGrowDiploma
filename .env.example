# Скопируйте в .env и заполните значения
# Секретный ключ: python -c "from django.core.management.utils import get_random_secret_key; print(get_random_secret_key())"
SECRET_KEY=your-secret-key-here
DEBUG=True
ALLOWED_HOSTS=localhost,127.0.0.1

DB_ENGINE=django.db.backends.postgresql
DB_NAME=leasegrow
DB_USER=postgres
DB_PASSWORD=your-password
DB_HOST=localhost
DB_PORT=5432

# Путь к папке bin PostgreSQL (pg_dump, psql) — на Windows, если не в PATH
# PG_BIN_PATH=C:\Program Files\PostgreSQL\18\bin